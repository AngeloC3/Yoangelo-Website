<div class="d-flex overflow-scroll flex-column">
    <h1>Users</h1>
    <% for (user of users){%>
        <div class="p-3 m-3 border border-primary bg-light">
            <h2><u>Username:</u>&emsp;<%= user.username %> </h2>
            <h4><u>Password:</u>&emsp;<%= user.passphrase %></h4>
            <h4><u>Other:</u>&emsp;<%= user.partner %></h4>                
        </div>
    <% } %>
</div>